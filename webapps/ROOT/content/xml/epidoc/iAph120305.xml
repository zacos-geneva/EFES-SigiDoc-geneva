<ab><lb n="1"/> <w lemma="θειότης">Θειότητι</w> <w lemma="αὐτοκράτωρ">Αὐτοκρατόρων</w> <w lemma="θεά">Θεᾷ</w> <persName type="divine"><name reg="Ἀφροδίτη">Ἀφροδίτῃ </name><w lemma="γενέτειρα">γεν<unclear reason="damage">ε</unclear><supplied reason="lost">τείρᾳ</supplied></w></persName>  <lb n="2"/> <w lemma="σύγκλητος">Συνκλήτῳ</w> <w lemma="δῆμος">Δήμῳ</w> <placeName reg="Ῥωμαῖος">Ῥωμαίων</placeName> <w lemma="πολίτης">πολείταις</w> <w lemma="εὐχαριστέω">εὐχαρισ<supplied reason="lost">τῶν</supplied></w>  <lb n="3"/> <persName type="aphrodisian"><name reg="Μᾶρκος">Μᾶρκος</name> <name reg="Ἰούλιος">Ἰούλιος</name> <name reg="Ἄτταλος">Ἄτταλος</name> <w lemma="ἀπελεύθερος">ἀπελεύθερος</w> <persName type="emperor"><name reg="Καῖσαρ">Καίσα<supplied reason="lost">ρος</supplied></name></persName></persName></ab>
