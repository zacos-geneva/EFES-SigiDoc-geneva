      <div type="description" n="monument">
        <head>Description of Monument</head>
        <p>Fragment from a <rs type="material" key="marmor">white marble</rs> <rs type="objectType">panel</rs>, with lower moulding
surviving (<measure type="length" unit="metre" dim="height">0.36</measure> ×
<measure type="length" unit="metre" dim="width">0.36</measure> × <measure type="length" unit="metre" dim="depth">0.10</measure>).</p>
      </div>

      <div type="description" n="text">
        <head>Description of Text</head>
        <p><rs type="execution" key="scalpro">Inscribed</rs> on the face.</p>
      </div>

      <div type="description" n="letters">
        <head>Description of Letters</head>
        <p>Tall, thinly cut; <measure type="length" unit="metre" dim="height">0.06-0.07</measure>.</p>
      </div>

      <div type="description" n="date">
        <head>Date</head>
        <p><date notAfter="0500" exact="none" notBefore="0301">Fourth-fifth century</date> (<rs type="criteria">lettering</rs>).</p>
      </div>

