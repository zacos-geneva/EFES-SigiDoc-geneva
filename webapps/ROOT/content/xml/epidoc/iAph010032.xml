      <div type="description" n="monument">
        <head>Description of Monument</head>
        <p>A <rs type="material" key="marmor">white marble</rs> <rs type="objectType">paving</rs> block.</p>
      </div>

      <div type="description" n="text">
        <head>Description of Text</head>
        <p>On the face.</p>
      </div>

      <div type="description" n="letters">
        <head>Description of Letters</head>
        <p>Av. <measure type="length" unit="metre" dim="height">0.04</measure>,
very worn.</p>
      </div>

      <div type="description" n="date">
        <head>Date</head>
        <p><date notAfter="0600" notBefore="0401" exact="none">Fifth-sixth century</date> or later (<rs type="criteria">content</rs>, <rs type="criteria">location</rs>).</p>
      </div>

      <div n="text" lang="grc" type="edition">
        <head lang="en">Edition</head>
        <ab><lb n="1"/><persName type="aphrodisian" key="george2" full="yes"><name>Γεώργ<unclear reason="damage">η</unclear>ος</name></persName><lb n="2"/><persName type="aphrodisian" full="yes"><name>Γηόργης</name></persName> <space dim="horizontal"/><orig n="unresolved">ρσκ</orig><lb n="3"/><orig n="unresolved"><unclear reason="damage">ιι</unclear></orig> <persName key="leon4" type="aphrodisian" full="yes"><name reg="Λέων">Λέον</name></persName> <orig n="unresolved"><unclear reason="damage">ιι</unclear></orig> <rs type="official-secul" key="domesticus"><w lemma="δομεστικός"><expan><abbr><unclear reason="damage">δ</unclear>ομ</abbr><supplied reason="abbreviation">εστικός</supplied></expan></w></rs><lb n="4"/><gap reason="lost" dim="left" extent="unknown" unit="character"/><orig n="unresolved">οσ</orig><gap reason="lost" dim="right" extent="unknown" unit="character"/></ab>	
      </div>

      <div type="apparatus">
        <head>Apparatus</head>
        <p>There are further traces of letters.</p>
      </div>

      <div type="translation">
        <head>Translation</head>
        <p>Georgios. Georgios. <space dim="horizontal"/> Leon, <seg cert="low"><term lang="la">dom<supplied reason="abbreviation">esticus</supplied></term></seg> <gap reason="lost" dim="left"/></p>
      </div>

