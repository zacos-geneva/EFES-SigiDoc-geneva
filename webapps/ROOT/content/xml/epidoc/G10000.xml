         <div type="apparatus">
            <head>Apparatus</head>
            <listApp>
               <app loc="1"><note><num><unclear cert="low">ρ</unclear></num> could be <num><unclear cert="low">π</unclear></num> or <num><unclear cert="low">πι</unclear></num>  </note></app>
               <app loc="4"><lem><unclear>Λ</unclear></lem> <note>could be</note> <rdg><unclear>A</unclear></rdg></app>
               <app loc="6"><note>D.-L. suggests ακ</note></app><!-- check -->
            </listApp>
