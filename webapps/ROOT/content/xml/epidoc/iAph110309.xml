      <div n="text" lang="grc" type="edition">
        <head lang="en">Edition</head>
		<ab>
		<lb n="0"/><gap dim="top" reason="lost" extent="unknown" unit="line"/>
		<lb n="1"/><gap dim="left" reason="lost" extent="unknown" unit="character"/><orig n="unresolved">χημα<unclear reason="damage">ι</unclear></orig><gap dim="right" reason="lost" extent="unknown" unit="character"/>
		<lb n="2"/><gap dim="left" reason="lost" extent="unknown" unit="character"/><orig n="unresolved"><unclear reason="damage">κ</unclear>αμηλ<unclear reason="damage">λ</unclear></orig><gap dim="right" reason="lost" extent="unknown" unit="character"/>
