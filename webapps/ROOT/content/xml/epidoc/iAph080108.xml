<ab><lb n="1"/> <w lemma="ὁ">τῇ</w> <persName type="divine" full="yes"><name reg="Ἀφροδίτη">Ἀφροδείτῃ</name></persName> <w lemma="καί">καὶ</w> <w lemma="ὁ">τοῖς</w> <persName type="emperor" key="anon" full="yes"><w lemma="σεβαστός">Σεβασ<lb n="2" type="worddiv"/>τοῖς</w></persName> <w lemma="ὁ">τὰ</w> <w lemma="ἀνάλημμα">ἀναλήματα</w> <w lemma="καί">καὶ</w> <w lemma="ὁ">τὰς</w> <lb n="3"/> <w lemma="ἄνοδος">ἀνόδους</w> <w lemma="καί">καὶ</w> <w lemma="ὁ">τὰ</w> <w lemma="σύστρωμα">συστρώματα</w> <lb n="4"/> <w lemma="καί">καὶ</w> <w lemma="ὁ">τὰς</w> <w lemma="κερκίς">κερκείδας</w> <w lemma="καί">καὶ</w> <w lemma="ὁ">τὰς</w> <w lemma="ψελίς">ψελί<lb n="5" type="worddiv"/>δας</w> <w lemma="καί">καὶ</w> <w lemma="ὁ">τὰ</w> <w lemma="βάθρον">βάθρα</w> <w lemma="πᾶς">πάντα</w> <space extent="3" unit="character" dim="horizontal"/>  <lb n="6"/><persName type="aphrodisian" full="yes"><name reg="Ἀριστοκλῆς">Ἀριστοκλῆς</name>  <name reg="Ἀρτεμίδωρος">Ἀρτεμιδώρου</name>  <lb n="7"/><name reg="Μολοσσός">Μολοσσὸς</name>  <name reg="Κάϊκος">Κάικος</name></persName> <w lemma="καί">καὶ</w> <persName type="aphrodisian" full="yes"><name reg="Κάϊκος">Κάικος</name>  <lb n="8"/><persName type="aphrodisian"><name reg="Ἀρτεμίδωρος">Ἀρτεμιδώρου</name></persName>  <name reg="Παπίας" type="">Παπίας</name></persName>    <g type="stop"/> <w lemma="ἐργεπιστατέω">ἐργε <lb n="9" type="worddiv"/>πιστατήσαντος</w>  <persName type="aphrodisian" full="yes"><name reg="Ἑρμᾶς" type="">Ἑρμᾶ</name></persName> <w lemma="κατά">κατὰ</w> <lb n="10"/> <w lemma="ὁ">τὰς</w> <persName type="aphrodisian" full="yes"><name reg="Μολοσσός">Μολοσσοῦ</name></persName> <w lemma="ὁ">τοῦ</w> <w lemma="τρέφω">θρέψαντος</w> <lb n="11"/> <w lemma="διαθήκη">διαθήκας</w> <w lemma="προσεπαγγέλλω">προσεπηγγελμέ<lb n="12" type="worddiv"/>νου</w> <w lemma="καί">καὶ</w> <w lemma="ὁ">τὸ</w> <w lemma="τρίτος">τρί<space extent="3" unit="character" dim="horizontal"/>τον</w> <w lemma="διάζωσμα">διάζωσμα</w> <lb n="13"/> <w lemma="ὁ">τοῦ