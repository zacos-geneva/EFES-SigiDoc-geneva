         <div type="apparatus">
            <head>Apparatus</head>
            <listApp>
               <app loc="1"><note>Only the initial Κ can now be read; Oliverio recorded the full name, but not the further letters.</note>
                  
               </app>
            <app loc="2">
                   <rdg resp="#SECir">Φλά<supplied reason="lost">μ</supplied>μα<unclear>ς</unclear></rdg>
               </app>
            
            </listApp>
