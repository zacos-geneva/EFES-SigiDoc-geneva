         <div type="description" n="monument">
            <head>Description of Monument</head>
            <p>A <rs type="material" key="marmor">white marble</rs> <rs type="objectType">column</rs> fluted above and smooth below.</p>
         </div>
         <div type="description" n="text">
            <head>Description of Text</head>
            <p>Cut on the smooth surface, <measure type="length" dim="none">0.145</measure> below the fluting.</p>
         </div>
         <div type="description" n="letters">
            <head>Description of Letters</head>
            <p>Letters: <measure dim="height" type="length" unit="metre">0.055</measure>-<measure dim="height" type="length" unit="metre">0.065</measure>; <foreign lang="grc">Ο</foreign> above line for abbreviation in l.2; ligature, <foreign lang="grc">ΜΕ</foreign> in l.1.</p>
         </div>
         <div type="description" n="date">
            <head>Date</head>
            <p><date exact="none" cert="low" notBefore="0368" notAfter="0400">Later fourth century A.D.</date>.</p>
         </div>
         <div n="text" lang="grc" type="edition">
            <head lang="en">Edition</head>
            <ab>
               <lb n="1"/><persName type="aphrodisian" key="Menander2" full="yes"><name reg="Μένανδρος"><hi rend="ligature">Με</hi>νάν<lb n="2" type="worddiv"/>δρου</name></persName> <w lemma="πολιτεύομαι"><expan><abbr>π<hi rend="superscript">ο</hi></abbr><supplied reason="abbreviation">λιτευομένου</supplied></expan></w>
