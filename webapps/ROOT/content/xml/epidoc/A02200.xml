            <div n="b" type="textpart">
               <ab>
                  <lb n="1"/><persName type="divine" key="asklepios"><name nymRef="Ἀσκλήπιος"><seg part="I">Ἀσκ<supplied reason="lost">λαπ</supplied></seg></name></persName><gap
