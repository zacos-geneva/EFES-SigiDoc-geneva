         <div type="edition" xml:lang="grc" xml:space="preserve"><div subtype="section" n="i" type="textpart"><ab>
                        <lb n="1"/><persName type="attested" ref="lgpn:V1-60198"><name nymRef="Ἴσχις">Ἴσχ<unclear>ι</unclear>ς</name>
            <lb n="2"/><persName type="attested" ref="lgpn:V1-60197"><name nymRef="Δίδυμος"><seg part="I">Διδυ<unclear>μ</unclear></seg></name></persName></persName><gap reason="lost" quantity="3" unit="character"/>
