          <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><persName type="attested"><name><seg part="F">άρχω</seg></name></persName>  <space extent="unknown" unit="character"/>
          <lb n="2"/><gap reason="lost" quantity="4" precision="low" unit="character"/> 
          <supplied reason="lost" cert="low"><w>χάριν</w></supplied> <w><supplied reason="lost">κ</supplied>αὶ</w> <w>τᾶς</w> <w>ποτὶ</w>	
          <lb n="3"/><supplied reason="lost"><w>τὰς</w> <w>θεὰς</w></supplied> 
            <w><supplied reason="lost">εὐ</supplied><unclear>σέ</unclear>βεας</w> <space extent="unknown" unit="character"/> 
          <lb n="4"/> <placeName type="ethnic"><w lemma="Κυραναῖος"><supplied reason="lost">Κυρ</supplied><unclear>ά</unclear>ν<unclear>α</unclear>ιοι</w></placeName>  
            <space extent="unknown" unit="character"/>
         </ab>
