      <div type="description" n="monument">
        <head>Description of Monument</head>
        <p>An Ionic <rs type="objectType">column capital</rs>, lying half buried. No measurements.</p>
      </div>

      <div type="description" n="text">
        <head>Description of Text</head>
        <p><rs type="execution" key="scalpro">Inscribed</rs> on two consecutive faces of the upper moulding
(abacus).</p>
      </div>

      <div type="description" n="letters">
        <head>Description of Letters</head>
        <p>No description; perhaps a small circle for stop.</p>
      </div>

      <div type="description" n="date">
        <head>Date</head>
        <p><date cert="low" exact="none" notBefore="0301" notAfter="0600">Early or middle Byzantine</date></p>
      </div>

      <div n="text" lang="grc" type="edition">
        <head lang="en">Edition</head>
        
        <ab>
          <lb n="1"/>
          <gap dim="left" reason="lost" unit="character" extent="unknown"/>
          <orig n="unresolved"><unclear reason="damage">λ</unclear>θ</orig>
          <gap dim="internal" extent="2" unit="character" reason="lost"/>
          <orig n="unresolved">οηβασι</orig>
          <gap dim="internal" extent="1" unit="character" reason="lost"/>
          <orig n="unresolved">ιαξοψ</orig>
          <g type="stop"/>
          <orig n="unresolved">τ</orig>
          <gap dim="internal" extent="2" unit="character" reason="lost"/>
          <orig n="unresolved">ν<unclear reason="damage">ο</unclear></orig>
          <gap reason="lost" unit="character" extent="unknown" dim="right"/>
        </ab>
      </div>

      <div type="translation">
        <head>Translation</head>
        <p>No words determinable</p>
      </div>

      <div type="commentary">
        <head>Commentary</head><p>See discussion at  <xref type="eAla-text" n="VII.24">VII.24</xref>.</p>
      </div>

