<ab><lb n="1"/><persName type="aphrodisian" full="yes"><name reg="Ὑψικλῆς">Ὑψικλῆς</name>  <persName type="aphrodisian"><name reg="Ἄδραστος">Ἀδράστου</name></persName></persName> <w lemma="ὁ">τὸ</w> <w lemma="ἐπιστύλιον">ἐπιστύλιον</w><lb n="2"/> <w lemma="καί">καὶ</w> <w lemma="ὁ">τὸν</w> <w lemma="ἐπί">ἐπ'</w> <w lemma="αὐτός">αὐτοῦ</w> <w lemma="κόσμος">κόσμον</w> <w lemma="ὁ">τῶι</w> <w lemma="δῆμος">Δήμωι</w></ab>
